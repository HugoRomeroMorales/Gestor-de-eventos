from dataclasses import dataclass, field
from typing import List, Dict, Optional
@dataclass
class persona:
    rol: str
    nombre: str
    apellido: str
    preferencias: List[str] = field(default_factory=list)
    
@dataclass
class mesa:
    numMesa: int
    numAsientos: int
    nombMesa: Optional[str] = None
    invitados = List[persona] = field(default_factory=list)
    invitadosNoAsignados = List[persona] = field(default_factory=list)
    
    
    


    